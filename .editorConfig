# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
[*]

end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

# Tab indentation (no size specified)
[Makefile,**.mk]
indent_style = tab
indent_size = 1
